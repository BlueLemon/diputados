<%= semantic_form_for @nota, :url => admin_notas_path do |f| %>
  <%= f.inputs "Detalles generales" do %>
    <%= f.input( :fojas ) %>
    <%= f.input( :iniciador ) %>
    <%= f.semantic_fields_for :pases do |pase| %>
      <%= pase.input :area %>
      <%= pase.input :ingreso, :as=>:string, :input_html => {:class => 'datepicker'} %>
      <%= pase.input :descripcion %>
    <% end %>
  <% end %>
  <%= f.actions %>
<% end %>
